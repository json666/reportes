	row["expedido"] =  jsonData.solicitud.cliente.persona.expedido;
	row["deudor"] =  jsonData.solicitud.cliente.persona.nombres;
	row["deudorAP"] =  jsonData.solicitud.cliente.persona.primerApellido;
	row["deudorAM"] =  jsonData.solicitud.cliente.persona.segundoApellido;
	row["direccion"] =  jsonData.solicitud.cliente.persona.domicilio.direccionDomicilio;
	row["nro"] =  jsonData.solicitud.cliente.persona.domicilio.nroDomicilio;
	row["zona"] =  jsonData.solicitud.cliente.persona.domicilio.zona;
	row["fecha"] =  jsonData.fechaCredito;	

//cambiamos el valor de la variable  a false.
	valor = false;
//indicamos que se tiene que imprimir esta parte
	return true;

}
//indicamos que ya no se tiene que imprimir.
return false;
]]></method>
